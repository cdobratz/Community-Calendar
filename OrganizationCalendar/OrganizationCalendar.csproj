<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyTitle>Organization Calendar Management System</AssemblyTitle>
    <AssemblyDescription>Calendar management system for SACH community with multiple houses</AssemblyDescription>
    <AssemblyCompany>SACH</AssemblyCompany>
    <AssemblyProduct>Organization Calendar</AssemblyProduct>
    <AssemblyCopyright>Copyright Â© 2025</AssemblyCopyright>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>



  <ItemGroup>
    <PackageReference Include="System.Data.SQLite" Version="1.0.119" />
  </ItemGroup>

</Project>
